<div class="panel panel-default">
<div class="panel-heading"> Details List </div>
  <div class="panel-body">

<p id="notice"><%= notice %></p>

<h1>Itemdetails</h1>

<table>
  <thead>
    <tr>
      <th>Itemattribute</th>
      <th>Itemvalue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @itemdetails.each do |itemdetail| %>
      <tr>
        <td><%= itemdetail.data %></td>
        <td><%= link_to 'Show', item_itemdetail_path(@item, itemdetail.id) %> | </td>
        <td><%= link_to 'Destroy', item_itemdetail_path(@item, itemdetail.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Itemdetail', new_item_itemdetail_path %>
</div>
</div>
